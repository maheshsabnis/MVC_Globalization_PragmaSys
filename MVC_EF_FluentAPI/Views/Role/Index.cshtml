@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "Index";
}

<h2>LIst of Roles</h2>
@Html.ActionLink("Create New Role", "Create")
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
		{
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
		}
    </tbody>
</table>

